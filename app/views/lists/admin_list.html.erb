<h1>Administraci√≥n de List</h1>
<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Titulo</th>
      <th>Detalle</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.nombre %></td>
        <td><%= list.titulo %></td>
        <td><%= list.detalle %></td>
        <td><%= list.url %></td>
        <td><%= link_to 'Show', list %></td>
        <td><%= link_to 'Edit', edit_list_path(list) %></td>
        <td><%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List', new_list_path %>
